{
  "name": "nascent-zodiac",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "qa:inventory": "npx tsx qa-i18n/scripts/route-inventory.ts",
    "qa:i18n": "npx tsx qa-i18n/scripts/i18n-coverage.ts",
    "qa:audit:fix": "npx tsx qa-i18n/scripts/audit-codebase.ts",
    "qa:i18n:fill": "npx tsx qa-i18n/scripts/i18n-fill-missing.ts",
    "qa:seo": "node scripts/seo-sanity.mjs",
    "perf:pw": "npx playwright test --config=playwright.perf.config.ts",
    "perf:report": "npx tsx scripts/generate-perf-report.ts",
    "qa:test": "npx playwright test -c qa-i18n/playwright.config.ts",
    "qa:full": "npm run qa:inventory && npx tsx qa-i18n/scripts/generate-urls.ts && npm run qa:audit:fix && npm run qa:i18n:fill && npm run qa:i18n && npm run qa:test",
    "gate:nav": "npx tsx scripts/quality-gates/validate-nav.ts",
    "validate:routes": "npx tsx scripts/validate_routes.ts",
    "gate:typecheck": "npx tsc --noEmit",
    "gate:lint": "npx eslint .",
    "gate": "node scripts/verify.mjs quick && npm run gate:nav && npm run validate:routes && npm run i18n:gate && npm run i18n:keys:gate && npm run gate:i18n:key-leak && npm run gate:hardcoded",
    "prebuild": "node scripts/check-secrets-hygiene.js",
    "build": "npm run gate && next build && npm run i18n:leak:gate",
    "start": "next start",
    "lint": "npx eslint .",
    "typecheck": "npx tsc --noEmit",
    "test": "vitest run",
    "format": "prettier --write .",
    "check:secrets": "node scripts/check-secrets-hygiene.js",
    "deploy:aws": "bash scripts/push-to-clouds.sh aws",
    "deploy:azure": "bash scripts/push-to-clouds.sh azure",
    "deploy:gcp": "bash scripts/push-to-clouds.sh gcp",
    "deploy:ocp": "bash scripts/push-to-clouds.sh ocp",
    "check:quick": "node scripts/verify.mjs quick",
    "check:build": "node scripts/verify.mjs build",
    "check:smoke": "node scripts/verify.mjs smoke",
    "check:i18n": "node scripts/verify.mjs i18n",
    "quality": "node scripts/verify.mjs all",
    "check:all": "npm run quality",
    "release:wordcount": "npx tsx scripts/check_wordcount.ts",
    "release:terms": "npx tsx scripts/check_forbidden_terms.ts",
    "release:gate:local": "node scripts/release_gate.mjs --mode=local",
    "release:gate:ci": "node scripts/release_gate.mjs --mode=ci",
    "seo:gate": "npm run build && node scripts/seo_gate.mjs",
    "perf:gate": "node scripts/perf_gate.mjs",
    "i18n:gate": "npx tsx scripts/i18n_gate.ts",
    "i18n:scan": "npx tsx scripts/i18n_scan.ts",
    "i18n:keys:gate": "npm run i18n:scan && node scripts/i18n_keys_gate.js",
    "i18n:qa": "npm run i18n:gate && npm run i18n:leak:gate && npm run gate:hardcoded",
    "i18n:leak:gate": "npx tsx scripts/i18n_leak_gate.ts",
    "gate:i18n:key-leak": "npx tsx scripts/i18n_no_key_leak_gate.ts",
    "gate:hardcoded": "npx tsx scripts/no_hardcoded_strings_gate.ts",
    "gate:diagrams": "node scripts/validate-diagrams.js",
    "security:gate": "node scripts/security_gate.mjs",
    "health:routes": "npx tsx scripts/check-routes.ts",
    "health:i18n": "npx tsx scripts/check-i18n-keys.ts",
    "validate:seo": "node scripts/validate-seo.mjs",
    "generate:pdfs": "node scripts/generate-paper-pdfs.js"
  },
  "dependencies": {
    "@auth/upstash-redis-adapter": "^2.11.1",
    "@swc/helpers": "^0.5.18",
    "@tailwindcss/postcss": "^4.1.18",
    "@upstash/redis": "^1.36.0",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "clsx": "^2.1.1",
    "glob": "^13.0.0",
    "lucide-react": "^0.562.0",
    "marked": "^17.0.1",
    "mermaid": "^11.12.2",
    "next": "^16.1.6",
    "next-auth": "^5.0.0-beta.30",
    "next-intl": "^4.6.1",
    "next-themes": "^0.4.6",
    "node-vault": "^0.10.9",
    "puppeteer": "^24.35.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-is": "^19.2.3",
    "recharts": "^3.6.0",
    "resend": "^6.6.0",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^16.1.1",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^20.19.27",
    "@types/node-vault": "^0.9.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "babel-plugin-react-compiler": "1.0.0",
    "class-variance-authority": "^0.7.1",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^27.4.0",
    "md-to-pdf": "^5.2.5",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.16"
  },
  "overrides": {
    "qs": "^6.14.1",
    "form-data": "^2.5.4"
  }
}
