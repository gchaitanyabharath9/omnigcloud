{
  "name": "nascent-zodiac",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "qa:i18n": "npx tsx qa-i18n/scripts/generate-tier1.ts && npx tsx qa-i18n/scripts/strict-i18n.ts",
    "qa:i18n:fill": "npx tsx qa-i18n/scripts/i18n-fill-missing.ts",
    "qa:seo": "node scripts/seo-sanity.mjs",
    "qa:e2e": "npx playwright test -c playwright.gate.config.ts",
    "qa:inventory": "npx tsx qa-i18n/scripts/route-inventory.ts",
    "qa:test": "npx playwright test -c qa-i18n/playwright.config.ts",
    "gate:nav": "npx tsx scripts/quality-gates/validate-nav.ts",
    "gate:typecheck": "tsc --noEmit",
    "gate:lint": "eslint .",
    "gate": "npm run gate:nav && npm run gate:typecheck && npm run gate:lint",
    "prebuild": "node scripts/check-secrets-hygiene.js && node scripts/verify.mjs quick",
    "build": "npm run gate && next build",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "format": "prettier --write .",
    "check:secrets": "node scripts/check-secrets-hygiene.js",
    "deploy:aws": "bash scripts/push-to-clouds.sh aws",
    "deploy:azure": "bash scripts/push-to-clouds.sh azure",
    "deploy:gcp": "bash scripts/push-to-clouds.sh gcp",
    "deploy:ocp": "bash scripts/push-to-clouds.sh ocp",
    "check:quick": "node scripts/verify.mjs quick",
    "check:build": "node scripts/verify.mjs build",
    "check:smoke": "node scripts/verify.mjs smoke",
    "check:i18n": "node scripts/verify.mjs i18n",
    "quality": "node scripts/verify.mjs all",
    "check:all": "npm run quality"
  },
  "dependencies": {
    "@auth/upstash-redis-adapter": "^2.11.1",
    "@swc/helpers": "^0.5.18",
    "@upstash/redis": "^1.36.0",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "next-auth": "^5.0.0-beta.30",
    "next-intl": "^4.6.1",
    "next-themes": "^0.4.6",
    "node-vault": "^0.10.9",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-is": "^19.2.3",
    "recharts": "^3.6.0",
    "resend": "^6.6.0",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.1",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^20.19.27",
    "@types/node-vault": "^0.9.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "1.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^27.4.0",
    "playwright": "^1.57.0",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.16"
  }
}
